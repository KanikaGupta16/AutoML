# ======================
# AutoML Backend Config
# ======================

# Required API Keys
OPENROUTER_API_KEY=your_openrouter_api_key
FIRECRAWL_API_KEY=your_firecrawl_api_key
KAGGLE_API_KEY=your_kaggle_api_key

# Optional: Firecrawl webhook secret
FIRECRAWL_WEBHOOK_SECRET=

# MongoDB Configuration
MONGODB_URI=mongodb+srv://user:pass@cluster.mongodb.net/db
MONGODB_DATABASE=automl

# Server Configuration
PORT=8000
BASE_URL=http://localhost:8000

# LLM Models (OpenRouter)
LLM_MODEL=google/gemini-2.0-flash-001
OPENROUTER_MODEL_INTENT=anthropic/claude-3.5-sonnet
OPENROUTER_MODEL_SCORE=meta-llama/llama-3.1-70b-instruct
